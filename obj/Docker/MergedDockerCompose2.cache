networks:
  backend:
    driver: bridge
services:
  db:
    container_name: GraphQL.SqlServer
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: test$$123
    image: mcr.microsoft.com/mssql/server
    networks:
      backend: {}
    ports:
    - published: 1433
      target: 1433
  graphql.users.api:
    build:
      context: C:\localRepository\2020_11_02_GraphQ
      dockerfile: GraphQL.Users.Api/Dockerfile
    container_name: GraphQL.Users.Api
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__UsersConnection: Server=db;Database=Users;User=sa;Password=test$$123;Integrated
        Security=False;MultipleActiveResultSets=True
    image: graphqlusersapi
    networks:
      backend: {}
    ports:
    - target: 80
    - target: 443
    volumes:
    - /c/Users/Paweł/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/Paweł/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'